<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="310dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="-64dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cast_mini_controller_progress_drawable" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorBottomNavigationNotification"
        android:padding="10dp"
        android:text="Gruppeområderum"
        android:textAlignment="center"
        android:textColor="@color/cast_expanded_controller_progress_text_color"
        android:textSize="20sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2">

    </TextView>

    <RatingBar
        android:id="@+id/ratingBar"
        style="@android:style/Widget.DeviceDefault.Light.RatingBar.Small"
        android:layout_width="82dp"
        android:layout_height="20dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        android:numStars="5"
        android:rating="3.5"
        android:textAlignment="textStart"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="176dp"
        android:layout_height="53dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:paddingLeft="50dp"
        android:paddingTop="15dp"
        android:text="Power"
        android:textAlignment="textStart"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="176dp"
        android:layout_height="53dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:paddingLeft="50dp"
        android:paddingTop="15dp"
        android:text="Group"
        android:textAlignment="textStart"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="166dp"
        android:layout_height="53dp"
        android:layout_marginLeft="-45dp"
        android:layout_marginTop="10dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:paddingLeft="50dp"
        android:paddingTop="15dp"
        android:text="Coffée"
        android:textAlignment="textStart"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@+id/ratingBar"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="166dp"
        android:layout_height="53dp"
        android:layout_marginLeft="0dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:paddingLeft="50dp"
        android:paddingTop="15dp"
        android:text="Wifi"
        android:textAlignment="textStart"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@+id/ratingBar"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="38dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ic_power_black_24px"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="38dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ic_wifi_black_24px"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="38dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ic_local_cafe_black_24px"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="38dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ic_group_black_24px"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/ic_navigation_black_24px"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/ic_place_black_24px"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/ratingBar" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adresse"
        android:textColor="@color/cast_expanded_controller_background_color"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text='"x"km væk'
        android:textColor="@color/cast_expanded_controller_background_color"
        app:layout_constraintLeft_toRightOf="@+id/imageView8"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>